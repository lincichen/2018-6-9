int ZS_SendFileData(long int end, int blk)
{
	int c, e;

	int blklen, maxblklen;

	if (Rxbuflen && (blk > Rxbuflen)) maxblklen = Rxbuflen;
	else maxblklen = blk;
	blklen = maxblklen;
	
	
SomeMore:
	if (Z_TestOneByte())
	{
WaitAck:
		switch (c = ZS_SyncWithReceiver())
		{
			default:	 
						 
						 
				return (-1);
			case 5:	 
						 
						 
				return c;
			case 3:	 
						 
						 
				break;
			case 9:	 
						 
						 
				break;
			case 1: 
						 
						 
				return 0;
		}
	}
	while (Z_TestOneByte())
	{
		switch (readline(1000))
		{
			case 0x18:
			case (-3):
			case '*':  goto WaitAck;
		}
	}

	Z_PutLongIntoHeader(Txpos);
	ZS_SendBinaryHeader(10, Txhdr);

	do
	{
		c = blklen;
		if ((Txpos + c) > end) 
			c = end - Txpos;
		if (c < blklen) 
			e = 'h';

		else e = 'i';

		ZS_SendData(Txpos, Txpos + c, e);
		Txpos += c;

		if (e == 'k') goto WaitAck;

		while (Z_TestOneByte())
		{
			switch (readline(1000))
			{
				case 0x18:
				case (-3):
				case '*':   
							 
							 
							 
					ZS_SendData(Txpos, Txpos, 'h');
					goto WaitAck;
			}
		}
	} while (e == 'i');

	while(1)
	{
		Z_PutLongIntoHeader(Txpos);
		ZS_SendBinaryHeader(11, Txhdr);

		switch (ZS_SyncWithReceiver())
		{
			case 3:		 
							 
							 
				continue;
			case 9:		 
							 
							 
				goto SomeMore;
			case 1:	 
							 
							 
				return 0;
			case 5:		 
							 
							 
				return c;
			default:		 
							 
							 
				return (-1);
		}
	}
}

 
 
 
 
