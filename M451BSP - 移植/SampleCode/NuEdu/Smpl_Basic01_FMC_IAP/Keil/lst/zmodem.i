int ZS_SyncWithReceiver()
{
	int c;
	int num_errs = 7;

	while(1)
	{
		c = Z_GetHeader(Rxhdr);

		switch (c)
		{
			case (-2):  if ((num_errs--) >= 0) break;
			case 16:
			case 7:
			case 8:
			case (-3):
						 
						 
						 
				return (-1);
			case 9:	 
						 
						 
				Txpos = Rxpos;
				return c;
			case 5:	 
						 
						 
			case 1: 
						 
						 
				return c;
			case 3:	 
						 
						 
				return c;
