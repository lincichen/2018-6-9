void ZS_SendBinaryHeader(int type, char *hdr)
{
	char *hptr;
	unsigned int crc;
	int  n;

	zmputchar('*');
	zmputchar(030);
	zmputchar('A');
	ZS_SendByte(type);

	crc = Z_UpdateCRC(type, 0);

	hptr  = hdr;
	for (n=4; --n >= 0;)
	{
		ZS_SendByte(*hptr);
		crc = Z_UpdateCRC(((unsigned short)(*hptr++)), crc);
	}

	crc = Z_UpdateCRC(0,crc);
	crc = Z_UpdateCRC(0,crc);
	ZS_SendByte(crc>>8);
	ZS_SendByte(crc);
}

 
 
 
 
